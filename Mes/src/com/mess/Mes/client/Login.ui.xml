<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	
.important {
		font-weight: bold;
	}
	

	</ui:style>
	<g:HTMLPanel>
	 <m:MaterialImage ui:field="h4" url="https://arabacademy-u8hapu3mdn.netdna-ssl.com/wp-content/uploads/2015/04/loginImage-400x300.jpg" width="50%" />
	<m:MaterialTextBox ui:field="name" label="First Name" iconType="FACE" ></m:MaterialTextBox>
     <m:MaterialTextBox ui:field="password" label="Password" iconType="MEMORY" ></m:MaterialTextBox>
	<m:MaterialButton ui:field="Login" text="LOGIN" iconType="LAPTOP" ></m:MaterialButton>
	</g:HTMLPanel>
</ui:UiBinder> 